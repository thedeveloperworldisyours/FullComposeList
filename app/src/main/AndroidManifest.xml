<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.a.jetpackcomposelists">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackComposeLists">
        <activity
            android:name=".main.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".section.SectionActivity"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar" />
        <activity
            android:name=".sectionstick.SectionStickActivity"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar" />
        <activity
            android:name=".expandable.ExpandableActivity"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar" />
        <activity
            android:name=".multiple.MultipleActivity"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar" />
        <activity
            android:name=".horizontal.HorizontalActivity"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar" />
        <activity
            android:name=".grid.GridActivity"
            android:theme="@style/Theme.JetpackComposeLists.NoActionBar" />
    </application>

</manifest>